<!DOCTYPE html>
<html lang="ch">
<head>
	<meta charset="UTF-8">
	<title>次要窗口</title>
	<script src="scmw_common.vbs" type="text/vbscript"></script>
	<script src="scmw_common.js" type="text/javascript"></script>
	<script type="text/javascript">
		window.onload = function() {
			document.onmousemove = SCMultiWindow.event.move;
			document.onmouseup = SCMultiWindow.event.up;
			document.onmousedown = SCMultiWindow.event.down;
			SCMultiWindow.initialize();
		}
		function check() {
			SCMultiWindow.magnetic = !SCMultiWindow.magnetic;
			alert(SCMultiWindow.WINDOW_ID);
		}
	</script>
	<style>
		* {
			padding: 0;
			margin: 0;
		}
		html, body {
			width: 100%;
			height: 100%;
			background: rgb(247, 99, 12);
			user-select: none;
		}
		#mainbox {
			padding: 10px;
			margin: 2px;
			position: absolute;
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;
			background: #fff;
		}
		#look {
			display: inline;
		}
	</style>
</head>
<body>
	<div id="mainbox">
		<b id="htano"></b>&nbsp;&nbsp;
		<b id="htaname"></b><br />
		<button onclick="window.close">close</button>
		<!-- <button onclick="SCMultiWindow.engaCalc">check</button> -->
		<button onclick="check">check</button>
		<br />
		<a id="con"></a>
	</div>
</body>
</html>